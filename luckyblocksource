local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Voidware | LUCKY BLOCKS Battlegrounds", HidePremium = true, SaveConfig = false, ConfigFolder = "Voidware"})

local Tab = Window:MakeTab({
	Name = "LB Spawning",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

